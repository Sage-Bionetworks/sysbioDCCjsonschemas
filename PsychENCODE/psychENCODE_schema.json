  "required": [
    "assay",
    "cellType",
    "consortium",
    "fileFormat",
    "grant",
    "group",
    "individualID",
    "individualIdSource",
    "investigatorName",
    "isStranded",
    "libraryID",
    "libraryPrep",
    "libraryPreparationMethod",
    "nucleicAcidSource",
    "organ",
    "passage",
    "platform",
    "postmortem",
    "readLength",
    "readPair",
    "referenceSet",
    "reportedGender",
    "reprogrammedCellType",
    "RIN",
    "runType",
    "sequencingReplicate",
    "species",
    "specimenID",
    "study",
    "technicalReplicate",
    "terminalDifferentiationPoint",
    "tissue"
  ],
  "allOf": [
    {
      "if": {
        "properties": {
          "species": {
            "const": "Human"
          }
        }
      },
      "then": {
        "required": ["sexChromosome", "race", "raceDetail", "genotypeInferredAncestry", "IQ", "BMI",
                     "primaryDiagnosis", "primaryDiagnosisDetail", "otherDiagnosis", "familyHistory",
                     "ageOnset", "neuropathDescription", "dementia", "otherMedicalDx"]
      }
    },
    {
      "if": {
        "allOf": [
          {
            "properties": {
              "species": {
                "const": "Human"
              },
              "otherDiagnosis": {
                "not": {"const": "None"}
              }
            },
	    "required": ["otherDiagnosis"]
          }
        ]
      },
      "then": {
        "required": ["otherDiagnosisDetail"]
      }
    },
    {
      "if": {
        "allOf": [
          {
            "properties": {
              "species": {
                "const": "Human"
              },
	      "dementia": {
                "const": true
              }
            },
            "required": ["dementia"]
          }
        ]
      },
      "then": {
        "required": ["CDR", "braak"]
      }
    },
    {
      "if": {
        "allOf": [
          {
            "properties": {
              "species": {
                "const": "Human"
              },
              "otherMedicalDx": {
                "not": {"const": "None"}
              }
            },
            "required": ["otherMedicalDx"]
          }
        ]
      },
      "then": {
        "required": ["otherMedicalDetail"]
      }
    },
    {
      "if": {
        "allOf": [
          {
            "properties": {
              "species": {
                "const": "Human"
              },
              "postmortem": {
                "const": "true"
              }
            },
	    "required": ["postmortem"]
          }
        ]
      },
      "then": {
        "required": ["ageOfDeath", "ageOfDeathUnits", "causeOfDeath", "mannerOfDeath", "postmortemTox",
	             "postmortemToxSource", "medRecordTox"]
      }
    },
    {
      "if": {
        "properties": {
          "postmortem": {
            "const": "true"
          }
        }
      },
      "then": {
        "required": ["PMICertain", "PMI", "pH", "organWeight", "organRIN", "yearAutopsy"]
      }
    }, 
    {
      "if": {
        "properties": {
          "organ": {
            "const": "brain"
          }
        }
      },
      "then": {
        "required": ["BrodmannArea"]
      }
    }, 
    {
      "if": {
        "properties": {
          "assay": {
            "const": "ChIPSeq"
          }
        }
      },
      "then": {
        "required": ["assayTarget", "FRiP"]
      }
    },
    {
      "if": {
        "properties": {
          "assay": {
            "const": "rnaSeq"
          }
        }
      },
      "then": {
        "required": ["rRNARate", "totalReads", "validBarcodeReads", "numberCells", "medianUMIs"]
      }
    },
    {
      "if": {
        "allOf": [
          {
            "properties": {
              "assay": {
                "const": "rnaSeq"
              },
              "platform": {
                "const": "10x"
              }
            }
          }
        ]
      },
      "then": {
        "required": ["medianGenes"]
      }
    },
    {
      "if": {
        "properties": {
          "assay": {
            "const": "ATACSeq"
          }
        }
      },
      "then": {
        "required": ["meanCoverage", "meanGCContent"]
      }
    },
    {
      "if": {
        "properties": {
          "fileFormat": {
            "anyOf": [
              {"const": "bai"},
              {"const": "bam"}
            ]
          }
        }
      },
      "then": {
        "required": ["percentAligned", "totalReads", "mappedReads"]
      }
    }
  ]
}
