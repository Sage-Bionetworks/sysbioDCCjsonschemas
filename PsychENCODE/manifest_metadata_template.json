{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"sysbio.metadataTemplates-pec.manifest",
    "description": "PsychENCODE manifest template schema",
    "properties": {
        "path": {
            "$ref": "sage.annotations-manifestKeys.path"
        },
        "parent": {
            "$ref": "sage.annotations-manifestKeys.parent"
        },
        "individualID": {
            "$ref": "sage.annotations-experimentalData.individualID"
        },
        "specimenID": {
            "$ref": "sage.annotations-experimentalData.specimenID"
        },
        "isMultiIndividual": {
            "$ref": "sage.annotations-experimentalData.isMultiIndividual"
        },
        "isMultiSpecimen": {
            "$ref": "sage.annotations-experimentalData.isMultiSpecimen"
        },
        "assay": {
            "$ref": "sage.annotations-experimentalData.assay"
        },
        "libraryID": {
            "$ref": "sage.annotations-ngs.libraryID"
        },
        "fileFormat": {
            "$ref": "sage.annotations-sageCommunity.fileFormat"
        },
        "consortium": {
            "const": "PEC"
        },
        "study": {
            "$ref": "sysbio.annotations-PECSpecific.study"
        },
        "grant": {
            "$ref": "sysbio.annotations-PECSpecific.grant"
        },
        "resourceType": {
            "$ref": "sage.annotations-sageCommunity.resourceType"
        },
        "dataType": {
            "$ref": "sage.annotations-experimentalData.dataType"
        },
        "dataSubtype": {
            "$ref": "sage.annotations-experimentalData.dataSubtype"
        },
        "metadataType": {
            "$ref": "sage.annotations-experimentalData.metadataType"
        },
        "analysisType": {
            "$ref": "sage.annotations-analysis.analysisType"
        }
    }
}
