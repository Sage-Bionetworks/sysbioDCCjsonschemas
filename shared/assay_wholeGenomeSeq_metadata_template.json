{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"sysbio.metadataTemplates-assay.wholeGenomeSeq",
    "description": "SysBio wholeGenomeSeq (aka WGS) metadata template schema",
    "properties": {
        "specimenID": {
            "$ref": "sage.annotations-experimentalData.specimenID"
        },
        "libraryID": {
            "$ref": "sage.annotations-ngs.libraryID"
        },
        "assay": {
            "$ref": "sage.annotations-experimentalData.assay"
        },
        "platform": {
            "$ref": "sage.annotations-experimentalData.platform"
        },
        "referenceSet": {
            "$ref": "sage.annotations-curatedData.referenceSet"
        },
        "dnaBatch": {
            "$ref": "sage.annotations-batch.dnaBatch"
        },
        "sequencingBatch": {
            "$ref": "sage.annotations-batch.sequencingBatch"
        },
        "libraryPrep": {
            "$ref": "sage.annotations-ngs.libraryPrep"
        },
        "libraryPreparationMethod": {
            "$ref": "sage.annotations-ngs.libraryPreparationMethod"
        },
        "readLength": {
            "$ref": "sage.annotations-ngs.readLength"
        },
        "runType": {
            "$ref": "sage.annotations-ngs.runType"
        },
        "totalReads": {
            "$ref": "sage.annotations-experimentalData.totalReads"
        },
        "numberCells": {
            "$ref": "sage.annotations-experimentalData.numberCells"
        },
        "ratio260over280": {
            "$ref": "sage.annotations-qc.ratio260over280"
        },
        "ratio260over230": {
            "$ref": "sage.annotations-qc.ratio260over230"
        },
        "GQN": {
            "$ref": "sage.annotations-qc.GQN"
        }
    }
}
