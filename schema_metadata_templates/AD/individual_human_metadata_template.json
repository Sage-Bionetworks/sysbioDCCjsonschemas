{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"sysbio.metadataTemplates-ad.individual.human",
    "description": "SysBio AD individual human metadata template schema",
    "properties": {
        "individualID": {
            "$ref": "sage.annotations-experimentalData.individualID"
        },
        "individualIdSource": {
            "$ref": "sage.annotations-experimentalData.individualIdSource"
        },
        "species": {
            "$ref": "sage.annotations-experimentalData.species"
        },
        "sex": {
            "$ref": "sage.annotations-experimentalData.sex"
        },
        "race": {
            "$ref": "sage.annotations-demographics.race"
        },
        "ethnicity": {
            "$ref": "sage.annotations-demographics.ethnicity"
        },
        "yearsEducation": {
            "$ref": "sage.annotations-demographics.yearsEducation"
        },
        "apoeGenotype": {
            "$ref": "sage.annotations-clinical.apoeGenotype"
        },
        "diagnosis": {
            "$ref": "sage.annotations-experimentalData.diagnosis"
        },
        "diagnosisCriteria": {
            "$ref": "sage.annotations-experimentalData.diagnosisCriteria"
        },
        "ageDeath": {
            "$ref": "sage.annotations-demographics.ageDeath"
        },
        "ageDeathUnits": {
            "$ref": "sage.annotations-demographics.ageDeathUnits"
        },
        "causeDeath": {
            "$ref": "sage.annotations-clinical.causeDeath"
        },
        "mannerDeath": {
            "$ref": "sage.annotations-clinical.mannerDeath"
        },
        "PMI": {
            "$ref": "sage.annotations-neuro.PMI"
        },
        "Braak": {
            "$ref": "sage.annotations-clinical.Braak"
        },
        "CERAD": {
            "$ref": "sage.annotations-clinical.CERAD"
        },
        "brainWeight": {
            "$ref": "sage.annotations-experimentalData.brainWeight"
        },
        "pH": {
            "$ref": "sage.annotations-neuro.pH"
        }
    },
    "required": [
        "individualID",
        "sex",
        "species"
    ]
}
